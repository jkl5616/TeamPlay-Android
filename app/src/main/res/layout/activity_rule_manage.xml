<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f0f0f0">

    <ListView
        android:id="@+id/lv_rule_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:dividerHeight="0dp"
        android:divider="@null"
        android:layout_margin="8dp"></ListView>

    <io.github.yavski.fabspeeddial.FabSpeedDial
        android:id="@+id/fab_main_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_margin="10dp"
        app:fabGravity="bottom_end"
        app:fabMenu="@menu/rule_fab_menu"
        app:miniFabBackgroundTint="@android:color/white"></io.github.yavski.fabspeeddial.FabSpeedDial>

</FrameLayout>